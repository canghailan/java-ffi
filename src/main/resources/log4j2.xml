<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout
                    pattern="%d %-5p [%t] %c{1.}:%L - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="cc.whohow" level="trace"/>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>